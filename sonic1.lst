00080918                            	dc.b	(\Offset>>15)&$FF	; 02h	- Start Bank
00080918                            	dc.b	(\Offset\_End>>15)&$FF	; 03h	- End Bank
00080918                            	z80word	(\Offset)|$8000		; 04h	- Start Offset (in Start bank)
00080918                            	z80word	(\Offset\_End-1)|$8000	; 06h	- End Offset (in End bank)
00080918                            	endm
